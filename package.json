{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "portfolio",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf ./docs && mkdir docs && sass --no-source-map ./src/css/main.scss ./docs/css/index.css && cp ./src/html/index.html ./docs/index.html && cp -r ./src/images ./docs/images && cp -r ./src/js ./docs/js && cp ./src/manifest_v1.webmanifest ./docs/manifest_v1.webmanifest",
    "build": "node ./sub.js ./docs/index.html ./docs/js/index.mjs ./docs/js/links.mjs ./docs/js/projects.mjs",
    "dev": "servor . ./src/html/index.html 3000 --reload",
    "lint": "eslint --fix ./src/js && echo 'Linted!'",
    "prepare": "snowpack",
    "sass": "sass --no-source-map --watch ./src/css/main.scss ./src/css/index.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/portfolio.git"
  },
  "author": "james south",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamessouth/portfolio/issues"
  },
  "homepage": "https://github.com/jamessouth/portfolio#readme",
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "sass": "^1.26.2",
    "servor": "^3.2.0",
    "snowpack": "^1.5.2"
  },
  "dependencies": {}
}
