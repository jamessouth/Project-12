{
  "name": "project-12",
  "version": "1.0.0",
  "description": "portfolio",
  "private": "true",
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie < 12"
  ],
  "scripts": {
    "lint": "eslint ./src/js",
    "analyze": "webpack --profile --json > stats.json",
    "dev": "webpack-dev-server",
    "build:prod": "webpack && cp src/favicon.ico docs/favicon.ico && cp src/manifest.json docs/manifest.json && cp -r src/images/icons/ docs/images/icons",
    "prebuild:prod": "rm -f docs/burst.min.js docs/worker.min.js && npx minify src/js/burst.js --out-file docs/burst.min.js && npx babel src/js/worker.js --presets=@babel/preset-env | npx uglifyjs -m toplevel -c toplevel,unsafe -o docs/worker.min.js && webpack --config sw-webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/Project-12.git"
  },
  "author": "james south",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamessouth/Project-12/issues"
  },
  "homepage": "https://github.com/jamessouth/Project-12#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "babel-loader": "^8.0.5",
    "babel-minify": "^0.5.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1",
    "workbox-webpack-plugin": "^4.1.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "web-animations-js": "^2.3.1"
  }
}
