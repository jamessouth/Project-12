{
  "name": "project-12",
  "version": "1.0.0",
  "description": "portfolio",
  "private": "true",
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie < 12"
  ],
  "scripts": {
    "lint": "eslint",
    "analyze": "webpack --profile --json > stats.json",
    "build": "webpack-dev-server",
    "build:prod": "webpack && npx html-minifier dist/index_pretty.html --collapse-whitespace --remove-script-type-attributes -o dist/index.html && rm dist/index_pretty.html",
    "prebuild:prod": "rm -f dist/burst.min.js dist/worker.min.js && npx minify src/js/burst.js --out-file dist/burst.min.js && npx babel src/js/worker.js --presets=@babel/preset-env | npx uglifyjs -m toplevel -c toplevel,unsafe -o dist/worker.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/Project-12.git"
  },
  "author": "james south",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamessouth/Project-12/issues"
  },
  "homepage": "https://github.com/jamessouth/Project-12#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "babel-loader": "^8.0.5",
    "babel-minify": "^0.5.0",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-minifier": "3.5.19",
    "html-webpack-plugin": "^3.2.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.23.1",
    "uglify-js": "^3.4.9",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "lodash-es": "^4.17.11",
    "web-animations-js": "^2.3.1"
  }
