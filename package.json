{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "portfolio",
  "private": "true",
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie < 12"
  ],
  "scripts": {
    "lint": "eslint ./src/js",
    "analyze": "webpack --profile --json > stats.json",
    "report": "webpack-bundle-analyzer stats.json docs/",
    "dev": "webpack-dev-server",
    "build": "webpack && cp src/favicon.ico docs/favicon.ico && cp src/manifest.webmanifest docs/manifest.webmanifest && cp -r src/images/icons/ docs/images/icons && cp src/sw.js.map docs/sw.js.map",
    "prebuild": "rm -f docs/BorderPaint.min.js docs/ButtonBG.min.js && npx minify src/js/BorderPaint.js --out-file docs/BorderPaint.min.js && npx minify src/js/ButtonBG.js --out-file docs/ButtonBG.min.js && webpack --config sw-webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/portfolio.git"
  },
  "author": "james south",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamessouth/portfolio/issues"
  },
  "homepage": "https://github.com/jamessouth/portfolio#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-minify": "^0.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "file-loader": "^4.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.0",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "workbox-webpack-plugin": "^4.3.1",
    "worker-plugin": "^3.2.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "core-js": "^3.2.1",
    "workbox-window": "^4.3.1"
  }
}
