{
  "name": "portfolio",
  "version": "2.0.0",
  "description": "portfolio",
  "private": true,
  "browserslist": [
    "last 2 versions",
    "> 1%",
    "not dead",
    "not ie < 12"
  ],
  "scripts": {
    "lint": "eslint --fix ./src/js && echo 'Linted!'",
    "analyze": "webpack --profile --json > stats.json",
    "report": "webpack-bundle-analyzer stats.json docs/",
    "dev": "webpack-dev-server --env.ENV=dev",
    "build": "webpack --env.ENV=prod && cp src/manifestv1.webmanifest docs/manifestv1.webmanifest && cp -r src/images/icons/ docs/images/icons",
    "prebuild": "rm -f docs/BorderPaint.min.js docs/ButtonBG.min.js && npx minify src/js/BorderPaint.js --out-file docs/BorderPaint.min.js && npx minify src/js/ButtonBG.js --out-file docs/ButtonBG.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessouth/portfolio.git"
  },
  "author": "james south",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jamessouth/portfolio/issues"
  },
  "homepage": "https://github.com/jamessouth/portfolio#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-minify": "^0.5.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^5.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "sass-loader": "^8.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "workbox-webpack-plugin": "^5.0.0",
    "worker-plugin": "^3.2.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "core-js": "^3.6.4",
    "workbox-window": "^5.0.0"
  }
}
